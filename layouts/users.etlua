<!doctype html>
<html>
<head>
	<meta charset="utf8">
	<title>tuat</title>
</head>
<body>
	<% for _, u in ipairs(this.users) do -%>
	<article>
		<header>
			<h3><%= u.name %></h3>
			<a href="<%= u.link %>"><%= u.link %></a>
		</header>
		<p><%= u.description or '' %></p>
		<hr>
	</article>
	<% end -%>
</body>
</html>
